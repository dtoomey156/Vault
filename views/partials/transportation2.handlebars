  <label for="vehicle-budget">Vehicle payment</label>
  <input type="text" id="vehicle-budget" class="btn-outline-primary cust-modal-input" value={{vehiclePay}} />

  <label for="gas-budget">Gas</label>
  <input type="text" id="gas-budget" class="btn-outline-primary cust-modal-input" value={{gas}} />

